{
  "rewrites": [
    {
      "source": "/server/:path*",
      "destination": "https://testmarkdb.azurewebsites.net/api/:path*"
    },
    {
      "source": "/server/(.*)",
      "destination": "https://testmarkdb.azurewebsites.net/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}