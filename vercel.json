{
  "rewrites": [
    {
      "source": "/test-google",
      "destination": "https://www.google.com/"
    },
    {
      "source": "/server/:match*/",
      "destination": "https://testmarkdb.azurewebsites.net/api/:match*/"
    },
    {
      "source": "/server/:match*",
      "destination": "https://testmarkdb.azurewebsites.net/api/:match*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}